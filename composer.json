{
  "name": "beastbytes/yii-tracy-panel-assets",
  "description": "Assets panel for Yii Tracy",
  "keywords": [
    "yii",
    "Tracy",
    "debugger",
    "assets"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "beastbytes/yii-tracy": "dev-main",
    "yiisoft/assets": "dev-master",
    "yiisoft/translator-message-php": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^6.0",
    "yiisoft/di": "^1.0",
    "yiisoft/dummy-provider": "^1.0",
    "yiisoft/test-support": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\Panel\\Assets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Yii\\Tracy\\Panel\\Assets\\Tests\\": "tests/"
    }
  },
  "extra": {
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "di-web": "di-web.php",
      "params-web": "params-web.php"
    }
  }
}
